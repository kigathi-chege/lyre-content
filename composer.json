{
  "name": "lyre/content",
  "description": "Pages and sections copy manager",
  "keywords": [
    "php",
    "github",
    "laravel",
    "Open Source",
    "lyre",
    "content",
    "sections",
    "pages",
    "crud",
    "relationships",
    "resources",
    "simple",
    "laravel 10",
    "laravel 11",
    "laravel 12"
  ],
  "license": "MIT",
  "version": "2.2.11",
  "minimum-stability": "stable",
  "autoload": {
    "files": [
      "src/utils/helpers.php"
    ],
    "psr-4": {
      "Lyre\\Content\\": "src/",
      "Lyre\\Content\\Database\\Factories\\": "database/factories/",
      "Lyre\\Content\\Database\\Seeders\\": "database/seeders/"
    },
    "classmap": [
      "src/"
    ]
  },
  "authors": [
    {
      "name": "kigathi-chege",
      "email": "chegekigathi@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "awcodes/filament-tiptap-editor": "^3.5",
    "filament/filament": "^3.3",
    "bezhansalleh/filament-shield": "202505041448.x-dev",
    "codeat3/blade-google-material-design-icons": "^1.20",
    "intervention/image-laravel": "^1.5",
    "valentin-morice/filament-json-column": "^2.0",
    "lyre/lyre": "^1.1",
    "lyre/file": "^1.0",
    "lyre/facet": "^1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lyre\\Content\\Providers\\LyreContentServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/kigathi-chege/filament-shield"
    }
  ]
}
